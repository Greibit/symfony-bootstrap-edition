{
    "name": "greibit/crud-generator",
    "description": "This bundle generates a nice CRUD",
    "keywords": ["crud", "generator"],
    "homepage": "https://github.com/greibit/CrudGeneratorBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "symfony/symfony": ">2.2.0, <2.5-dev",
        "sensio/generator-bundle": ">2.2.0, <2.4-dev",
        "pagerfanta/pagerfanta": "1.0.*@dev",
        "lexik/form-filter-bundle": "2.0.*",
        "doctrine/orm": ">=2.2.3,<2.5-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "Greibit\\Bundle\\CrudGeneratorBundle": "" }
    },
    "target-dir": "Greibit/CrudGeneratorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
